<aura:component controller="UNILAB_PAL_Controller" implements="flexipage:availableForRecordHome,force:lightningQuickActionWithoutHeader,force:appHostable,lightning:actionOverride,force:hasRecordId" access="global"  >
	<aura:attribute name="recordId" type="String" />
    
    <header class="slds-modal__header">
        <p class="slds-text-heading_medium slds-text-align_center">
            CLONE ALLOCATION
        </p>
    </header>
    <div style="max-height: 150px; height: 150px; overflow-y: auto !important;">
        <p>Are you sure you want to clone this record?</p>
    </div>
    <footer class="slds-modal__footer" style="padding: 0.50rem 1rem;">
        <lightning:button variant="brand" label="Clone"  onclick="{!c.cloneAllocation}" />
        <lightning:button variant="neutral" label="Cancel"  onclick="{!c.cancelAllocation}" />
    </footer>
    
</aura:component>