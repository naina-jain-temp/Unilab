                    <section role="alertdialog" tabindex="-1" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal_prompt" aria-modal="true">
                        <div class="slds-modal__container">
                            <header class="slds-modal__header slds-theme_info slds-theme_alert-texture">
                                <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                                    
                                    <span class="slds-assistive-text">{!$Label.c.Work_Plan_Modal_Menu_Close}</span>
                                </button>
                                <h2 class="slds-text-heading_medium" id="prompt-heading-id">{!$Label.c.Work_Plan_Modal_Title_Confirmation}</h2>
                            </header>
                            <div class="slds-modal__content slds-p-around_medium slds-text-align_center" id="prompt-message-wrapper">
                                <h2>Do you want to save unsaved changes?</h2>                        
                            </div>
                            <footer class="slds-modal__footer slds-theme_default">
                                
                                <button class="slds-button slds-button_neutral"  onclick="{!c.handleSaveUnsaved}">{!$Label.c.Work_Plan_Modal_Button_Yes}</button>
                                
                                <button class="slds-button slds-button_neutral" onclick="{!c.handleCancelSaveUnsaved}">{!$Label.c.Work_Plan_Modal_Button_Cancel}</button>
                                
                            </footer>
                        </div>
                    </section>
                    <div class="slds-backdrop slds-backdrop_open"></div>
                </lightning:layoutItem>            
            </aura:if>
            
            
            <!-- SUBMISSION CONFIRMATION -->
            <aura:if isTrue="{!v.showSubmitConfirm}">
                <lightning:layoutItem flexibility="grow" size="12" smallDeviceSize = "12" mediumDeviceSize="12" largeDeviceSize="12" class="slds-p-right_large slds-p-left_large">
                    <section role="alertdialog" tabindex="-1" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal_prompt" aria-modal="true">
                        <div class="slds-modal__container">
                            <header class="slds-modal__header slds-theme_info slds-theme_alert-texture">
                                <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close">
                                    
                                    <span class="slds-assistive-text">{!$Label.c.Work_Plan_Modal_Menu_Close}</span>
                                </button>
                                <h2 class="slds-text-heading_medium" id="prompt-heading-id">{!$Label.c.Work_Plan_Modal_Title_Confirmation}</h2>
                            </header>
                            <div class="slds-modal__content slds-p-around_medium slds-text-align_center" id="prompt-message-wrapper">
                                <h2>Are you sure you want to submit TCR for {! v.tcr.DFS__c == null ? v.tcr.CDS_Trainee__r.Name : v.tcr.DFS__r.Name }?</h2>                        
                            </div>
                            <footer class="slds-modal__footer slds-theme_default">
                                
                                <button class="slds-button slds-button_neutral"  onclick="{!c.handleUpdateStatus}">{!$Label.c.Work_Plan_Modal_Button_Yes}</button>
                                
                                <button class="slds-button slds-button_neutral" onclick="{!c.handleCancelSubmit}">{!$Label.c.Work_Plan_Modal_Button_Cancel}</button>
                                
                            </footer>
                        </div>
                    </section>
                    <div class="slds-backdrop slds-backdrop_open"></div>
                </lightning:layoutItem>            
            </aura:if>
        </aura:if> 
    </div>
</aura:component>