<aura:component controller="UNILAB_TrainingContactController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,lightning:actionOverride" access="global"  >    
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="currentStep" type="String" default="1" />
    <aura:attribute name="tlEvent" type="Event" />
    <aura:attribute name="tcr" type="Training_Contact_Report__c" />
    <aura:attribute name="tcrCalls" type="List" default="{}" />   
    <aura:attribute name="accountTcr" type="String" default="Account TCR"  access="private"/>    
    <aura:attribute name="t3Tcr" type="String" default="T3 TCR" access="private"/>
    <aura:attribute name="subdAuditTcr" type="String" default="SUBD AUDIT TCR" access="private"/>
    <aura:attribute name="meetingTcr" type="String" default="Meeting TCR" access="private"/>
    <aura:attribute name="callsSection" type="String" default="7" access="private"/>
    
    <aura:attribute name="Draft" type="String" default="Draft" access="private"/>
    <aura:attribute name="PendingConcur" type="String" default="Pending Concurrence" access="private"/>
    <aura:attribute name="pendingApproval" type="String" default="Pending Approval" access="private"/>
    
    <aura:attribute name="tcrFieldLabelMap" type="Map" default="{'':''}" />
    <aura:attribute name="tcrRequiredFieldMap" type="Map" default="{'':''}" />
    <aura:attribute name="tcrCallFieldInfoMap" type="Map" default="{'':''}" />
    <aura:attribute name="tcrCallRequiredFields" type="List" default="{}" />
    <aura:attribute name="sectionOneValue" type="String" default="0" />
    <aura:attribute name="stepToValidate" type="String" default="0" /> 
    <aura:attribute name="specificSkillDev1List" type="List" default="[]" /> 
    <aura:attribute name="specificSkillDev2List" type="List" default="[]" /> 
    
    <aura:attribute name="isValidUser" type="Boolean" default="false" />
    <aura:attribute name="isDisabled" type="Boolean" default="true" />
    <aura:attribute name="isWithPreviousTcr" type="Boolean" default="false" />
    <aura:attribute name="isRequired" type="Boolean" default="false" />    
    <aura:attribute name="isSectionValid" type="Boolean" default="false" />           
    <aura:attribute name="validateCallSection" type="Boolean" default="false" />
    <aura:attribute name="isWithUnsaved" type="Boolean" default="false" />    
    <aura:attribute name="isAllowSave" type="Boolean" default="true" />      
    <aura:attribute name="isWithCalls" type="Boolean" default="false" />      
    
    <aura:attribute name="renderTradeTcr" type="Boolean" default="false" />
    <aura:attribute name="showSubmitConfirm" type="Boolean" default="false" />
    <aura:attribute name="showUnsaveConfirm" type="Boolean" default="false" />
    <aura:attribute name="isDraft" type="Boolean" default="true" /> 
    
    <aura:attribute name="toastMessage" type="String" default="" />
    <aura:attribute name="errorMessage" type="String" default="" />
    <aura:attribute name="showNotifToast" type="Boolean" default="false" />
    <aura:attribute name="showErrorMessage" type="Boolean" default="false" />
    
    <aura:attribute name="legendSectionThree" type="Boolean" default="false"/>
    <aura:attribute name="legendSectionFour" type="Boolean" default="false"/>
    <aura:attribute name="spinner" type="boolean" default="false"/>
    
    <!-- <aura:attribute name="meetingTCRActiveSections" type="List" default="['A', 'B']" />
    <aura:attribute name="subAuditTCRActiveSections" type="List" default="['A', 'B', 'C']" /> -->
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />  
    
    <ltng:require styles="{! $Resource.disableRefresh }" />
    
    
    <div id="errormessage" class="slds container" aura:id="containerDiv">
        <aura:if isTrue="{!v.showErrorMessage}">
            <div  class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_error" role="alert">
                <h2>{!v.errorMessage}</h2>
            </div>
        </aura:if>
        <aura:if isTrue="{!v.showNotifToast}">
            
            <lightning:layout horizontalAlign="spread" >
                
                <lightning:layoutItem  flexibility="auto" size="12" smallDeviceSize = "4" mediumDeviceSize="12" largeDeviceSize="12" >
                    <div  class=" slds-notify slds-theme_success slds-align_absolute-center" role="alert" style="padding:10px;">
                        <span class="slds-assistive-text">success</span>
                        <span class="slds-icon_container slds-icon-utility-success slds-m-right_small slds-no-flex slds-align-top" title="Description of icon when needed">
                            <lightning:icon variant="inverse"  size="small" iconName="utility:success" alternativeText="Success" />                        
                        </span>
                        <div class="slds-notify__content">
                            <h2 class="slds-text-heading_small ">{!v.toastMessage}</h2>
                        </div>                    
                    </div>
                </lightning:layoutItem>
            </lightning:layout>  
        </aura:if>
        
        <aura:if isTrue="{! v.tcr != null}" >
            <div id="tcrForm" style="height: 100%; margin: 0; overflow:auto;">
                <aura:if isTrue="{!v.spinner}">
                    <div aura:id="spinnerId" class="slds-spinner_container">
                        <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                            <span class="slds-assistive-text">Loading</span>
                            <div class="slds-spinner__dot-a"></div>
                            <div class="slds-spinner__dot-b"></div>
                        </div>
                    </div>
                </aura:if>
                
                <div style="height: 100%; display: flex; flex-direction: column;">
                    <!-- PAGE HEADER -->
                    <div class="slds-page-header" style="background-color: white;">
                        <div class="slds-media">
                            <div class="slds-media__figure">
                                <span class="slds-avatar slds-avatar_large">
                                    <img src="{! $Resource.tcricon2 }" />
                                </span>
                            </div>
                            <div class="slds-media__body">
                                <h1 class="slds-page-header__title slds-truncate slds-align-middle slds-p-bottom_xx-small">
                                    Welcome, 
                                    <strong>
                                        {! v.tcr.DFS__c == null  ? v.tcr.Team_Leader_Trainer__r.Name : v.tcr.CDS_Trainee__r.Name }
                                    </strong>
                                </h1>
                                <p class="slds-text-body_small slds-line-height_reset slds-p-bottom_xx-small">
                                    {! v.isDraft ? 'Creating TCR for: ' : 'Created TCR for: '}
                                    <strong>{! v.tcr.DFS__c == null  ? v.tcr.CDS_Trainee__r.Name : v.tcr.DFS__r.Name }</strong>
                                </p>
                                <p class="slds-text-body_small slds-line-height_reset slds-p-bottom_xx-small">
                                    TCR Type:
                                    <strong>
                                        {! v.tcr.RecordType.Name }
                                    </strong>
                                </p>
                                <p class="slds-text-body_small slds-line-height_reset slds-p-bottom_xx-small">
                                    TCR Status:
                                    <strong>
                                        {! v.tcr.Status__c }<br/>                                        
                                    </strong>
                                </p>
                                <p class="slds-text-body_small slds-line-height_reset">
                                    
                                </p>
                            </div>
                        </div>                
                    </div>
                    <div class="slds-p-bottom_x-small" />
                    
                    <!-- TCR MAIN COMPONENT -->
                    
                    <article class="slds-card slds-scrollable_y" style="height: 100%;">
                        <div class="slds-card__body slds-card__body_inner">
                            <!-- LIGHTNING PROGRESS INDICATOR -->
                            <lightning:layout multipleRows="true" verticalAlign="stretch">
                                <lightning:layoutItem size="12" flexibility="auto, grow" padding="around-small">
                                    <lightning:progressIndicator currentStep="{! v.currentStep }" type="base" variant="base">
                                        <lightning:progressStep label="Home" value="1" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Basics" value="2" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 1" value="3" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 2A" value="4" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 2B" value="5" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 2C" value="6" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 3" value="7" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 4" value="8" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Section 5" value="9" onclick="{! c.goToSection }" />
                                        <lightning:progressStep label="Summary" value="10" onclick="{! c.goToSection }" />
                                    </lightning:progressIndicator>
                                </lightning:layoutItem>
                            </lightning:layout>
                            <div class="slds-p-top_medium" />
                            
                            <!-- WELCOME SCREEN -->
                            <div class="{! v.currentStep == '1' ? 'slds-show' : 'slds-hide' }">
                                <img src = "{! $Resource.unilablogocircle }" class="slds-align_absolute-center" />
                                <h1 class="slds-page-header__super-title slds-text-align_center slds-p-bottom_x-large"
                                    style="font-size: 2rem;">
                                    {! $Label.c.TCR_Title }
                                </h1>
                                <aura:if isTrue="{! v.isDraft}">
                                    <p class="slds-text-align_center" style="color: blue;">
                                        {! $Label.c.TCR_Intro_Subtitle_1 }
                                    </p>
                                    
                                    <p class="slds-text-align_center slds-p-bottom_small" style="color: blue;">
                                        {! $Label.c.TCR_Intro_Subtitle_2 }
                                    </p>
                                </aura:if>
                                <div class="slds-align_absolute-center">
                                    <lightning:button variant="brand" label="{! v.isDraft ?$Label.c.TCR_Button_Begin  : $Label.c.TCR_Button_Next}" 
                                                      title="Begin"
                                                      onclick="{! c.handleBegin }"  />
                                </div>
                            </div>
                            <!-- END WELCOME SCREEN -->
                            
                            <!-- BASIC INFORMATION -->
                            <div class="{! v.currentStep == '2' ? 'slds-show' : 'slds-hide' }">
                                <h1 class="slds-page-header__super-title slds-text-align_center slds-p-bottom_x-large"
                                    style="font-size: 1.25rem;">
                                    {! $Label.c.TCR_Basic_Information }
                                </h1>
                                <lightning:layout multipleRows="true">
                                    <aura:if isTrue="{! v.tcr.Team_Leader_Trainer__c != null }">
                                        <lightning:layoutItem class="slds-p-bottom_small" size="12" padding="horizontal-large" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="12">
                                            <lightning:input aura:id="team_leader_trainer__c" name="tl" value="{! v.tcr.Team_Leader_Trainer__r.Name}" class="slds-p-bottom_small" disabled="true"/>
                                        </lightning:layoutItem>
                                    </aura:if>
                                    <lightning:layoutItem class="slds-p-bottom_small" size="12" padding="horizontal-large" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="12">
                                        <lightning:input aura:id="cds_trainee__c" name="cds" value="{! v.tcr.CDS_Trainee__r.Name}" class="slds-p-bottom_small" disabled="true"/>
                                    </lightning:layoutItem>
                                    <aura:if isTrue="{! v.tcr.DFS__c != null}">
                                        <lightning:layoutItem class="slds-p-bottom_small" size="12" padding="horizontal-large" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="12">
                                            <lightning:input aura:id="dfs__c" name="tl" value="{! v.tcr.DFS__r.Name}" class="slds-p-bottom_small" disabled="true"/>
                                        </lightning:layoutItem>
                                    </aura:if>
                                    <lightning:layoutItem class="slds-p-bottom_small" size="12" padding="horizontal-large" smallDeviceSize="12" mediumDeviceSize="6" largeDeviceSize="6">
                                        <lightning:input aura:id="date__c" name="date" value="{! v.tcr.Date__c}" class="slds-p-bottom_small" disabled="true"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem class="slds-p-bottom_small" size="12" padding="horizontal-large" smallDeviceSize="12" mediumDeviceSize="6" largeDeviceSize="6">
                                        <lightning:input aura:id="area__c" name="area" label="Area:" value="{! v.tcr.Area__c}" class="slds-p-bottom_small" disabled="{! v.isDisabled}" 
                                                         onchange="{! c.handleChangeValue}"/> 
                                    </lightning:layoutItem>
                                </lightning:layout>
                            </div>
                            <!-- END BASIC INFORMATION -->
                            
                            <!-- SECTION 1: PLANNING -->
                            <div class="{! v.currentStep == '3' ? 'slds-show' : 'slds-hide' }">
                                
                                <h1 class="slds-page-header__super-title slds-text-align_center slds-p-bottom_x-large"
                                    style="font-size: 1.25rem;">
                                    {! $Label.c.TCR_Section_1 }
                                </h1>
                                <h2 class="slds-page-header__super-title slds-text-align_center slds-p-bottom_x-large">
                                    {! $Label.c.TCR_Planning_Subtitle }
                                </h2>
                                <lightning:layout class="slds-p-bottom_small" multipleRows="true">
                                    <lightning:layoutItem size="12" padding="horizontal-large" smallDeviceSize="12" 
                                                          mediumDeviceSize="{! $Browser.formFactor == 'DESKTOP' ? '12' : '6' }" 
                                                          largeDeviceSize="6">
                                        <lightning:radioGroup class="customRadioLabel" aura:id="sales_target__c" name="salesTarget"
                                                              value="{! v.tcr.Sales_Target__c }"
                                                              disabled="{! v.isDisabled }" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="sales_target_notes__c" name="salesTargetNotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.Sales_Target_Notes__c }"
                                                            disabled="{! v.isDisabled }"  onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="st_to_date_performance__c" name="salesTargetToDatePerformance"
                                                              value="{! v.tcr.ST_To_Date_Performance__c }"
                                                              disabled="{! v.isDisabled }" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="st_to_date_performance_notes__c" name="stToDatePerformanceNotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.ST_To_Date_Performance_Notes__c}"
                                                            disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="st_to_date_per_performance__c" name="salesTargetToDatePerPerformance"
                                                              value="{! v.tcr.ST_To_Date_Per_Performance__c}"
                                                              disabled="{! v.isDisabled}" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="st_to_date_per_performance_notes__c" name="stToDatePerPerformanceNotes"
                                                            placeholder="" maxlength="255" 
                                                            value="{! v.tcr.ST_To_Date_Per_Performance_Notes__c}"
                                                            disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="availability_rps_target_prep__c" name="rpsTarget"
                                                              value="{! v.tcr.Availability_RPS_Target_Prep__c }"
                                                              disabled="{! v.isDisabled }" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="availability_rps_target_prep_notes__c" name="availrpstargetnotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.Availability_RPS_Target_Prep_Notes__c }"
                                                            disabled="{! v.isDisabled }" onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="presence_visibility_standards__c" name="visibilityStandards"
                                                              value="{! v.tcr.Presence_Visibility_Standards__c }"
                                                              disabled="{! v.isDisabled }" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="presence_visibility_standards_notes__c" name="visibilityStandardsNotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.Presence_Visibility_Standards_Notes__c }"
                                                            disabled="{! v.isDisabled }" onchange="{! c.handleChangeValue}"/>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem size="12" padding="horizontal-large" smallDeviceSize="12" 
                                                          mediumDeviceSize="{! $Browser.formFactor == 'DESKTOP' ? '12' : '6' }"
                                                          largeDeviceSize="6">
                                        <lightning:radioGroup class="customRadioLabel" aura:id="collection_target__c" name="collection_target__c"
                                                              value="{! v.tcr.Collection_Target__c}"
                                                              disabled="{! v.isDisabled}" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="collection_target_notes__c" name="collectionTargetNotes"
                                                            placeholder="" maxlength="255" 
                                                            value="{! v.tcr.Collection_Target_Notes__c}" 
                                                            disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="ct_to_date_performance__c" name="colTargetToDatePerformance"
                                                              value="{! v.tcr.CT_To_Date_Performance__c}"
                                                              disabled="{! v.isDisabled}" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="ct_to_date_performance_notes__c" name="ctToDatePerformanceNotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.CT_To_Date_Performance_Notes__c}"
                                                            disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/> 
                                        <lightning:radioGroup class="customRadioLabel" aura:id="ct_to_date_per_performance__c" name="colTargetToDatePerPerformance"
                                                              value="{! v.tcr.CT_To_Date_Per_Performance__c}"
                                                              disabled="{! v.isDisabled}" type="button" onchange="{! c.handleChangeValue}"/>
                                        <lightning:textarea aura:id="ct_to_date_per_performance_notes__c" name="ctToDatePerPerformanceNotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.CT_To_Date_Per_Performance_Notes__c}" 
                                                            disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="npd_saturation_prep__c" name="npdPrep"
                                                              value="{! v.tcr.NPD_Saturation_Prep__c}"
                                                              disabled="{! v.isDisabled}" type="button" onchange="{! c.handleChangeValue}"/>    
                                        <lightning:textarea aura:id="npd_saturation_prep_notes__c" name="npdPrepNotes" placeholder="" maxlength="255" 
                                                            value="{! v.tcr.NPD_Saturation_Prep_Notes__c}" disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/>
                                        <lightning:radioGroup class="customRadioLabel" aura:id="channel_initiatives_prep__c" name="channelInitiativePrep"
                                                              value="{! v.tcr.Channel_Initiatives_Prep__c}"
                                                              disabled="{! v.isDisabled}" type="button" onchange="{! c.handleChangeValue}"/>    
                                        <lightning:textarea aura:id="channel_initiatives_prep_notes__c" name="channelInitiativePrepNotes"
                                                            placeholder="" maxlength="255"
                                                            value="{! v.tcr.Channel_Initiatives_Prep_Notes__c}"
                                                            disabled="{! v.isDisabled}" onchange="{! c.handleChangeValue}"/>
                                    </lightning:layoutItem>
                                </lightning:layout>
                            </div>
                            <!-- END SECTION 1: PLANNING -->
                            
                            <!-- SECTION 2A: SALES TOOLS -->
                            <div class="{! v.currentStep == '4' ? 'slds-show' : 'slds-hide' }">
                                <h1 class="slds-page-header__super-title slds-text-align_center slds-p-bottom_x-large"
                                    style="font-size: 1.25rem;">
                                    {! $Label.c.TCR_Section_2A }
                                </h1>
                                <lightning:layout class="slds-p-bottom_small" multipleRows="true">
                                    <!-- ACCOUNT AND T3 TCR -->                                  
                                    <aura:if isTrue="{! or(v.tcr.RecordType.Name == v.accountTcr, v.tcr.RecordType.Name == v.t3Tcr) }">
                                        <lightning:layoutItem size="12" padding="horizontal-large" smallDeviceSize="12" 
                                                              mediumDeviceSize="{! $Browser.formFactor == 'DESKTOP' ? '12' : '6' }"
                                                              largeDeviceSize="6">
                                            <lightning:radioGroup aura:id="sfa_tablet__c" name="sfa_tablet" type="button"
                                                                  disabled="{! v.isDisabled }" value="{! v.tcr.SFA_Tablet__c }"
                                                                  class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>
                                            <aura:if isTrue="{! v.tcr.RecordType.Name == v.t3Tcr }">
                                                <lightning:radioGroup aura:id="check_pjp_dfs_cdr_wfar__c" name="Check_workplan"
                                                                      value="{! v.tcr.Check_PJP_DFS_CDR_WFAR__c }"
                                                                      disabled="{! v.isDisabled }" type="button"
                                                                      class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>                              
                                                <aura:set attribute="else">
                                                    <lightning:radioGroup aura:id="check_workplan_cds_coverage__c" name="Check_workplan"
                                                                          value="{! v.tcr.Check_Workplan_CDS_Coverage__c }"
                                                                          disabled="{! v.isDisabled }" type="button"
                                                                          class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>
                                                </aura:set>
                                            </aura:if>
                                            <lightning:radioGroup class="customRadioLabel" aura:id="review_route_plan__c" name="Review route plan"
                                                                  value="{! v.tcr.Review_Route_Plan__c }"
                                                                  disabled="{! v.isDisabled }" type="button" onchange="{! c.handleChangeValue}"/>
                                        </lightning:layoutItem>
                                        <lightning:layoutItem size="12" padding="horizontal-large" smallDeviceSize="12" 
                                                              mediumDeviceSize="{! $Browser.formFactor == 'DESKTOP' ? '12' : '6' }"
                                                              largeDeviceSize="6">
                                            <lightning:radioGroup aura:id="top_psf__c" name="top"
                                                                  value="{! v.tcr.TOP_PSF__c }"
                                                                  disabled="{! v.isDisabled }" type="button"
                                                                  class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>
                                            <lightning:radioGroup aura:id="ics_availability_check__c" name="ics" 
                                                                  value="{! v.tcr.ICS_Availability_Check__c }"
                                                                  disabled="{! v.isDisabled }" type="button"
                                                                  class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>
                                            <lightning:radioGroup aura:id="official_receipt_collection_documents__c" name="or"
                                                                  value="{! v.tcr.Official_Receipt_Collection_documents__c }"
                                                                  disabled="{! v.isDisabled }" type="button"
                                                                  class="customRadioLabel" onchange="{! c.handleChangeValue}"/>
                                        </lightning:layoutItem>
                                    </aura:if>
                                    <!-- SUBD AUDIT TCR -->
                                    <aura:if isTrue="{! v.tcr.RecordType.Name == v.subdAuditTcr}">
                                        <lightning:layoutItem size="12" padding="horizontal-large" smallDeviceSize="12" 
                                                              mediumDeviceSize="{! $Browser.formFactor == 'DESKTOP' ? '12' : '6' }"
                                                              largeDeviceSize="6">                                
                                            <lightning:radioGroup aura:id="sfa_tablet__c" name="sfa_tablet"
                                                                  type="button" disabled="{! v.isDisabled }"
                                                                  value="{! v.tcr.SFA_Tablet__c }"
                                                                  class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>                                 
                                            <lightning:radioGroup aura:id="check_workplan_cds_coverage__c" name="Check_workplan"
                                                                  value="{! v.tcr.Check_Workplan_CDS_Coverage__c }"
                                                                  disabled="{! v.isDisabled }" type="button"
                                                                  class="customRadioLabel" onchange="{! c.handleChangeValue}"/>                                                                    
                                        </lightning:layoutItem>
                                        <lightning:layoutItem size="12" padding="horizontal-large" smallDeviceSize="12" 
                                                              mediumDeviceSize="{! $Browser.formFactor == 'DESKTOP' ? '12' : '6' }"
                                                              largeDeviceSize="6">
                                            <lightning:radioGroup aura:id="review_route_plan__c" name="Review route plan"
                                                                  value="{! v.tcr.Review_Route_Plan__c }"
                                                                  disabled="{! v.isDisabled }" type="button"
                                                                  class="customRadioLabel slds-m-bottom_small" onchange="{! c.handleChangeValue}"/>
                                            <lightning:radioGroup aura:id="official_receipt_collection_documents__c" name="or"
