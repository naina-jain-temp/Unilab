<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>51.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>UNILAB Brand Bank Products</masterLabel>
    <targets>
        <target>lightning__AppPage</target>
        <target>lightning__RecordPage</target>
        <target>lightning__UtilityBar</target>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <!-- Configuring the design attributes -->
    <targetConfigs>
        <targetConfig targets="lightning__AppPage, lightningCommunity__Default">
            <property name="title" type="String" label="Title" default="UNILAB Brand Bank Products" required="true"/>
            <!--<property name="recordId" type="String" default="recordId" label="Record Id" required="true"/>-->
            <property name="showDetails" type="Boolean" default="true" label="Do you want to Show Details ?"/>
            <property name="showsync" type="Boolean" default="true" label="User can sync the record from Salesforce?" />
            <property name="usedInCommunity" type="Boolean" default="false" label="Component is used in Community?" />
            <property name="communityName" type="String" default="none" label="Community Name" />
            <property name="reportId" type="String" default="00O7F00000BB0RaUAL" label="Report Id" required="true"/>
            <property name="documentId" type="String" default="0691e000000KjxXAAS" label="Document Id" required="true"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>